<div class="row-fluid">
  <div class="span12">
    <%= link_to 'Sign out', destroy_user_session_path, :method => :delete %>
  </div>
</div>
<div class="row-fluid">
  <div class="span12">
    <%= link_to 'New event', new_event_path %>
